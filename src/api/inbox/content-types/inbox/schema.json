{
  "kind": "collectionType",
  "collectionName": "inboxes",
  "info": {
    "singularName": "inbox",
    "pluralName": "inboxes",
    "displayName": "Inbox",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Message": {
      "type": "text"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "store": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::store.store"
    },
    "Archived": {
      "type": "boolean"
    },
    "parentMessage": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::inbox.inbox",
      "inversedBy": "childMessages"
    },
    "childMessages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inbox.inbox",
      "mappedBy": "parentMessage"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
