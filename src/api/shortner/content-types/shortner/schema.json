      "required": true
    },
    "visit": {
      "type": "integer",
      "default": 0
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    }
  }
