{
  "kind": "collectionType",
  "collectionName": "subscribers",
  "info": {
    "singularName": "subscriber",
    "pluralName": "subscribers",
    "displayName": "Subscriber",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Email": {
      "type": "email",
      "required": true
    },
    "active": {
      "type": "boolean"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "stores": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::store.store"
    },
    "lists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::subscriber.subscriber-list",
      "mappedBy": "subscribers"
    },
    "EmailVerified": {
      "type": "datetime"
    },
    "extensions": {
      "type": "component",
      "repeatable": true,
      "component": "common.extension"
    },
    "newsletter_preferences": {
      "type": "json",
      "default": {
        "subscribed": true,
        "frequency": "weekly",
        "topics": [],
        "tags": []
      }
    },
    "unsubscribed_at": {
      "type": "datetime"
    },
    "sendgrid_contact_id": {
      "type": "string"
    },
    "sendgrid_list_ids": {
      "type": "json",
      "default": []
    },
    "last_synced_at": {
      "type": "datetime"
    },
    "sync_status": {
      "type": "enumeration",
      "enum": [
        "pending",
        "synced",
        "failed",
        "unsubscribed"
      ],
      "default": "pending"
    }
  }
}
